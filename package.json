{
  "name": "@avesta-oss/avesta-sms-gateway",
  "version": "0.0.4",
  "description": "configurable sms gateway",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "tsc && node ./lib/index.js",
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "sms",
    "gateway",
    "2factor",
    "textlocal",
    "msg91"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/team-avesta/avesta-sms-gateway.git/issues"
  },
  "homepage": "https://github.com/team-avesta/avesta-sms-gateway.git#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/team-avesta/avesta-sms-gateway.git"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^0.21.1"
  },
  "directories": {
    "lib": "lib"
  }
